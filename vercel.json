{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node@20.x",
      "config": {
        "maxDuration": 60,
        "memory": 3008
      }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}